{
  "name": "markdown-utils",
  "description": "Micro-utils for creating markdown snippets.",
  "version": "0.7.0",
  "homepage": "https://github.com/jonschlinkert/markdown-utils",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/markdown-utils",
  "bugs": {
    "url": "https://github.com/jonschlinkert/markdown-utils/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "array-slice": "^0.2.3",
    "is-number": "^2.1.0",
    "list-item": "^1.1.1",
    "to-gfm-code-block": "^0.1.1"
  },
  "devDependencies": {
    "for-own": "^0.1.2",
    "gulp-format-md": "^0.1.8"
  },
  "keywords": [
    "blog",
    "commonmark",
    "markdown",
    "md",
    "post",
    "remarkable",
    "render",
    "renderer",
    "util",
    "write"
  ],
  "verb": {
    "related": {
      "list": [
        "gfm-code-blocks",
        "markdown-link",
        "markdown-toc",
        "remarkable"
      ]
    },
    "reflinks": [
      "bullets",
      "verb"
    ],
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
