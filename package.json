{
  "name": "markdown-utils",
  "description": "Tiny helpers for creating consistenly-formatted markdown snippets.",
  "version": "1.0.0",
  "homepage": "https://github.com/jonschlinkert/markdown-utils",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/markdown-utils",
  "bugs": {
    "url": "https://github.com/jonschlinkert/markdown-utils/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "is-number": "^7.0.0",
    "list-item": "^2.0.0"
  },
  "devDependencies": {
    "gulp-format-md": "^1.0.0",
    "mocha": "^5.2.0"
  },
  "keywords": [
    "blog",
    "commonmark",
    "markdown",
    "markdown-it",
    "md",
    "post",
    "remark",
    "remarkable",
    "text",
    "utils",
    "write"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "gfm-code-blocks",
        "markdown-link",
        "markdown-toc",
        "remarkable"
      ]
    },
    "reflinks": [
      "bullets",
      "fill-range",
      "remarkable"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
